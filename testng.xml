<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TVOLearnTestSuite" parallel="tests" thread-count="2">
  <test name="Chrome Tests" allow-return-values="true">
	<parameter name="browserName" value="chrome" />
		<classes>
			<class name="testpackage.TVOLearnTests" />
		</classes>
	</test>
	<test name="Firefox Tests" allow-return-values="true">
	<parameter name="browserName" value="firefox" />
		<classes>
			<class name="testpackage.TVOLearnTests" />
		</classes>
	</test>
</suite> <!-- TVOLearnTestSuite -->
